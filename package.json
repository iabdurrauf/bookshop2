{
  "name": "bookshop",
  "version": "1.0.0",
  "description": "A simple CAP project.",
  "repository": "<Add your repository here>",
  "license": "UNLICENSED",
  "private": true,
  "dependencies": {
    "@sap/cds": "^6",
    "@sap/cds-hana": "^2.0.0",
    "@sap/xssec": "^3.2.18",
    "express": "^4",
    "hdb": "^0.19.0",
    "passport": "^0"
  },
  "devDependencies": {
    "@sap/hdi-deploy": "4.6.1",
    "@sap/ux-specification": "^1.108.10",
    "rimraf": "^3.0.2",
    "sqlite3": "^5"
  },
  "scripts": {
    "start": "cds-serve",
    "watch-browsecatalog": "cds watch --open browsecatalog/webapp/index.html?sap-ui-xx-viewCache=false",
    "undeploy": "cf undeploy bookshop --delete-services --delete-service-keys --delete-service-brokers",
    "build": "rimraf resources mta_archives && mbt build --mtar archive",
    "deploy": "cf deploy mta_archives/archive.mtar --retries 1"
  },
  "cds": {
    "hana": {
      "deploy-format": "hdbtable"
    },
    "requires": {
      "db": {
        "kind": "hana"
      },
      "CE_PRODUCT_0002": {
        "kind": "odata",
        "model": "srv/external/CE_PRODUCT_0002"
      }
    }
  },
  "sapux": [
    "app/browsecatalog"
  ]
}